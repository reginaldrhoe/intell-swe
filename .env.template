# Copy this file to .env and fill in secrets locally (DO NOT commit .env)

# OpenAI API key (optional - deterministic embedding fallback will be used when missing)
OPENAI_API_KEY=

# Qdrant URL used by the services (when running with docker-compose this is usually the service name)
QDRANT_URL=http://qdrant:6333

# Default collection name for RAG ingestion
RAG_COLLECTION=rag-poc

# Any other environment variables you use locally
# EXAMPLE_VAR=value
